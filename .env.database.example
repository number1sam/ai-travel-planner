# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/travel_agent_db
DATABASE_SSL=false

# Provider API Keys (for real integrations)
BOOKING_API_KEY=your_booking_api_key
EXPEDIA_API_KEY=your_expedia_api_key
GETYOURGUIDE_API_KEY=your_getyourguide_api_key
VIATOR_API_KEY=your_viator_api_key
ZOMATO_API_KEY=your_zomato_api_key
SKYSCANNER_API_KEY=your_skyscanner_api_key
AMADEUS_API_KEY=your_amadeus_api_key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Application Configuration
NODE_ENV=development
PORT=3000
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Security
SESSION_SECRET=your_session_secret_here
JWT_SECRET=your_jwt_secret_here

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true

# Cache Configuration
REDIS_URL=redis://localhost:6379
CACHE_TTL_HOURS=24

# External Services
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key